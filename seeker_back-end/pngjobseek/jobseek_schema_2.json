{
  "name": "JobListings",
  "baseSelector": "tr.evenrow, tr.oddrow",
  "fields": [
    {
      "name": "title",
      "selector": "h2.tablewidth",
      "type": "text"
    },
    {
      "name": "location",
      "selector": "h3:contains(\", \")",
      "type": "text"
    },
    {
      "name": "datePosted",
      "selector": "h3.h3-blue",
      "type": "text"
    },
    {
      "name": "jobLink",
      "selector": "a.viewDetails, a[href*='display-job']",
      "type": "attribute",
      "attribute": "href"
    },
    {
      "name": "description",
      "selector": "td.viewDetails p",
      "type": "text"
    }
  ]
}